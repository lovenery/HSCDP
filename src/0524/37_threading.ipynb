    "import threading\n",
    "def count(i):\n",
    "    print ('這是一個數字 : ',i)\n",
    "threads = []\n",
    "for i in range(0,10):\n",
    "    threads.append(threading.Thread(target=count, args=(i,)))\n",
    "for t in threads:\n",
    "    t.start()\n",
    "\n"
   ]
